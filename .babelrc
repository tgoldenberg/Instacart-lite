{
  "plugins": [
    ["transform-class-properties"],
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "components": "./components",
        "server": "./server",
        "store": "./store",
        "hocs": "./hocs",
        "styles": "./styles"
      },
      "cwd": "babelrc"
    }],
    ["wrap-in-js", {
      "extensions": ["css$", "scss$"]
    }]
  ],
  "presets": [
    "next/babel",
    "es2015"
  ],
  "ignore": []
}
